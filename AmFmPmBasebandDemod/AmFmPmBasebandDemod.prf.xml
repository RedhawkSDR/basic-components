<!DOCTYPE properties PUBLIC "-//JTRS//DTD SCA V2.2.2 PRF//EN" "properties.dtd">
<properties>
  <simple id="freqDeviation" mode="readwrite" type="double">
    <description>FM signal frequency deviation</description>
    <value>0.0</value>
    <units>Hz</units>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="squelch" mode="readwrite" type="double">
    <description>Any sample's voltage which falls under the squelch threshold is set to zero.</description>
    <value>-150</value>
    <units>dB</units>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="phaseDeviation" mode="readwrite" type="double">
    <description>PM signal phase deviation</description>
    <value>1.0</value>
    <units>cycles</units>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
  <simple id="debug" mode="readwrite" type="boolean">
    <value>False</value>
    <kind kindtype="configure"/>
    <action type="external"/>
  </simple>
</properties>
